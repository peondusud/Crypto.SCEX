function showHelp(helpSubject)
{
  Help = window.open('http://www.voyages-sncf.com/dynamic/_SvTermComm2?_DLG=SvTermComm2&_CMD=cmdHelp&_LANG=FR&_AGENCY=VSC&hf_help=' + helpSubject, 'Help','status=no,width=425,height=300,scrollbars');
}


function showHelpOnPage()
{
	var _form = document.saisie ; 
	var _HelpOn = _form.hf_help ; 
  Help = window.open('http://www.voyages-sncf.com/dynamic/_SvTermComm2?_DLG=SvTermComm2&_CMD=cmdHelp&_LANG=FR&_AGENCY=VSC&hf_help=' + _HelpOn.value, 'Help','status=no,width=425,height=300,scrollbars');
}


function frequentTravelSelected(liste)
{
	document.location = 'http://www.voyages-sncf.com/dynamic/_SvTermComm2?_DLG=SvTermComm2&_CMD=CMD_FREQUENT_TRAVEL_CHOOSEN&_LANG=FR&_AGENCY=VSC&regularTravel=' + liste.options[liste.selectedIndex].value
}


