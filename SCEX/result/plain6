function createFrequentTravel(paramUrl) {
  if (confirm("Attention ! Vous serez enregistré comme unique passager du voyage que vous venez de commander. Si vous désirez ajouter des compagnons à ce voyage, nous vous invitons à consulter votre compte. Merci."))
  	document.location = paramUrl ;
}

function showMaximiles()
{
	window.open('http://www.maximiles.com/joinsncf.php');
}
